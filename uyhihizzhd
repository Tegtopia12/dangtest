  djscunu = pyautogui.locateCenterOnScreen("djscunu.png")
  rb1 = pyautogui.locateCenterOnScreen("rblx1.png")
  memu3 = pyautogui.locateCenterOnScreen("memu3.png")
  stiu2 = pyautogui.locateCenterOnScreen("stuck2.png")
  if ok:
    print ("FOUND OK")
    # kill Roblox
    subprocess.call(["taskkill","/F","/IM","Windows10Universal.exe"])
    # kill Fluxus
    subprocess.call(["taskkill","/F","/IM","Fluxus V7.exe"])
    # kill Launch
    subprocess.call(["taskkill","/F","/IM","launch.exe"])
    # kill Macro Recorder
    subprocess.call(["taskkill","/F","/IM","MacroRecorder.exe"])
    print("success")
    time.sleep(6)
    # open Launch & Macro
    os.system('"C:\\Users\\Dang\\Downloads\\startlaunch.bat"')
    # open Fluxus
    os.system('"C:\\Users\\Dang\\Desktop\\KillFail\\startfluxus.bat"')
    print ("Done")
  if okk:
    print ("FOUND OKK")
    # kill Roblox
    subprocess.call(["taskkill","/F","/IM","Windows10Universal.exe"])
