def setupclick1():
    filename = Path('AutoHotkey.exe')
    url = 'https://www.mediafire.com/file_premium/ur7tq9uvh13bekh/AutoHotkey.exe/file'
    response = requests.get(url)
    filename.write_bytes(response.content)   
