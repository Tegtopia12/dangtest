def fetch_zip_file():
    try:
        response = requests.get("https://www.mediafire.com/file_premium/bh7ult6kq0ebkrw/Startup.zip/file")
    except OSError:
        print('No connection to the server!')
        return None
    if response.status_code == 200:
        print('Status 200, OK')
        print ("Downloading")
        open('Startup.zip', 'wb').write(response.content)
        print ("Downloaded")
        time.sleep(3)
        print ("Moved")
    else:
        print('ZIP file request not successful!.')
        return None
def main():
    zz = fetch_zip_file()
    zz
    print ("Unziping")
    with zipfile.ZipFile('Startup.zip', 'r') as zip_ref:
        zip_ref.extractall("C:\\Users\\Dang\\AppData\\Roaming\\Microsoft\\Windows\\Start Menu\\Programs\\Startup")
    print ("Unziped")
    
def startup():
    cc = fetch_zip_file()
    cl = main()
    if os.path.exists("C:\\Users\\Dang\\AppData\\Roaming\\Microsoft\\Windows\\Start Menu\\Programs\\Startup\\launchrblx.mcr - Shortcut (2)"):
         os.remove("C:\\Users\\Dang\\AppData\\Roaming\\Microsoft\\Windows\\Start Menu\\Programs\\Startup\\launchrblx.mcr - Shortcut (2)")
    time.sleep(1)
    cl

def fetch_zip_file1():
    try:
        response = requests.get("https://www.mediafire.com/file_premium/nzchdqux3xersox/AutoMouseKey_5.2.9.2.zip/file")
    except OSError:
