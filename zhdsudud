    thread_count = len(GetDevices())
    print(thread_count)
    
def randomstring(n):
    return ''.join(random.choice(string.ascii_lowercase + string.digits) for _ in range(n))

def random_number(n):
    return ''.join(random.choice(string.digits) for _ in range(n))

